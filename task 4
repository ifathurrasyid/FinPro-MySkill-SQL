select
	rank () over(order by count(distinct od.id)desc) as rank,
	pd.payment_method as "payment method",
	count(distinct od.id) as count
from
	order_detail od
join
	payment_detail pd
on
	od.payment_id = pd.id 
where 
	od.is_valid = 1
and
	extract (year from order_date) = 2022
group by
	"payment method"
order by 
	count desc
limit
	5
;
